{
  "name": "lbda-jikko-planner-pdf",
  "version": "1.0.0",
  "description": "Lambda function for count registers",
  "private": true,
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "build:qa": "npm run build --NODE_ENV=QA",
    "build:st": "npm run build --NODE_ENV=STG",
    "build:prod": "npm run build --NODE_ENV=PROD",
    "start": "npm run build && export DEBUG=bdb:* && node ./dist/bin/www.js",
    "dev": "nodemon --watch src -e ts,js --exec npm start",
    "test": "jest --passWithNoTests",
    "test:only": "jest",
    "coverage": "jest --coverage",
    "coverage:report": "jest --coverage",
    "lint": "eslint '*/**/*.{js,ts,tsx}'",
    "lint:fix": "npx eslint '*/**/*.{js,ts}' --fix",
    "prettier:all": "prettier \"**/*.{ts,js,json,html,scss,md}\" --write",
    "prettier": "prettier \"**/*.{ts,js,json,html,scss,md}\" --check",
    "fix:all": "npm run prettier:all && npm run lint:fix",
    "sonar:local": "npm run coverage:report && npm run clean && sonar-scanner",
    "debug": "DEBUG=* node ./dist/bin/www.js",
    "clean": "rm -rf dist .scannerwork",
    "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\""
  },
  "keywords": [
    "javascript",
    "js",
    "node-js"
  ],
  "author": "Jikko",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-database-migration-service": "^3.911.0",
    "@aws-sdk/client-dynamodb": "^3.911.0",
    "@aws-sdk/client-sfn": "^3.952.0",
    "aws-sdk": "^2.1130.0",
    "aws-serverless-express": "^3.4.0",
    "axios": "^0.26.1",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "express": "^4.17.3",
    "express-openapi-validate": "^0.6.1",
    "install": "^0.13.0",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "openpgp": "^5.11.0",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.159",
    "@types/chai": "^4.3.1",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.2.3",
    "@types/mocha": "^9.1.1",
    "@types/morgan": "^1.9.3",
    "@types/nock": "^11.1.0",
    "@types/node": "^17.0.31",
    "@types/sinon": "^10.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "aws-sdk-client-mock": "^4.1.0",
    "aws-sdk-mock": "^6.2.0",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "eslint": "^8.11.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "jest": "^29.7.0",
    "mocha": "^9.2.2",
    "nock": "^13.2.4",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0",
    "prettier": "^2.6.1",
    "sinon": "^13.0.2",
    "sonarqube-scanner": "^2.8.1",
    "ts-node": "^10.7.0",
    "tslint": "^6.1.3",
    "typescript": "^4.9.5"
  }
}
