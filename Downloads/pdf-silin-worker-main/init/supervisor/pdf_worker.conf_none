[program:pdf_worker]
command = /bin/bash /app/compose/supervisor/pdf_worker_start.sh
user = root
stdout_logfile = /app/compose/supervisor/pdf_worker.log
redirect_stderr = true
environment=LANG=en_US.UTF-8,LC_ALL=en_US.UTF-8
autostart=true
autorestart=true
stopsignal=INT
stopasgroup=true
killasgroup=true
